        <activity
            android:name=".activities.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.StudentsList"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.StudentDetails"
            android:screenOrientation="portrait" />
